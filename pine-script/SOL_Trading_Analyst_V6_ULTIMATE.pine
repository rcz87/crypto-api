    calculate_volume_profile()
    
    current_signal := sig
    entry_price := ent
    stop_loss := sl_val
    take_profit := tp_val
    confidence := conf_val
    confluence_score := conf_score
    
    [whale_activity, vol_spike, large_candle, mega_whale] = detect_whale_activity()
    whale_detected := whale_activity

