                 color=color.new(color.green, 40), width=2, style=line.style_dashed)
        label.new(bar_index, pivot_low, "S", color=color.new(color.green, 70), 
                  textcolor=color.white, size=size.normal, style=label.style_label_up)

// ===== V6 ENHANCED HEATMAP =====
if show_heatmap
