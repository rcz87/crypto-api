      }
    },
    "/api/sol/position-calculator": {
      "post": {
        "operationId": "calculatePosition",
        "summary": "Position calculator with risk analysis",
        "tags": ["SOL Trading"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "entryPrice": {"type": "number"},
                  "size": {"type": "number"},
                  "leverage": {"type": "integer"},
                  "side": {"type": "string", "enum": ["long", "short"]},
                  "accountBalance": {"type": "number"}
                }
              }
            }
          }
        }
      }
    },
    "/api/sol/risk-dashboard": {
      "post": {
        "operationId": "getRiskDashboard",
        "summary": "Portfolio risk management dashboard",
        "tags": ["SOL Trading"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "positions": {
                    "type": "array",
                    "items": {"type": "object"}
                  },
                  "accountBalance": {"type": "number"},
                  "riskLimits": {"type": "object"}
                }
              }
            }
          }
        }
      }
    },
    "/api/sol/premium-orderbook": {
      "get": {
        "operationId": "getPremiumOrderbook",
        "summary": "Premium orderbook metrics (VIP tiers)",
        "tags": ["Premium"]
      }
    },
    "/api/premium/institutional-analytics": {
      "get": {
        "operationId": "getInstitutionalAnalytics",
